<%- include('partials/header'); -%>
    <h1>Home</h1>
    <p class="pp">
        <%=homeStartingContent%>
    </p>
    <% posts.forEach((e)=> {%>

        <h1>
            <%= e.title %>
        </h1>
        <p class="pp">
            <%= e.textPost.substring(0, 101) + "..." %>
                <a href="/posts/<%=e.title.toLowerCase().split(" ").join("-")%>">Read More</a>
        </p>
        <% });%>
            <%- include('partials/footer'); -%>